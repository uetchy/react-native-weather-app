{
  "expo": {
    "name": "Weather",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "slug": "react-native-weather-app",
    "ios": {
      "supportsTablet": true
    },
    "platforms": ["ios"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"]
  }
}
