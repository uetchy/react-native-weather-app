{
  "expo": {
    "name": "Weather",
    "slug": "react-native-weather-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios"],
    "ios": {
      "supportsTablet": true
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "osmKey": "<OpenWeatherMap API Key>"
    }
  }
}
